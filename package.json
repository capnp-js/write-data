{
  "name": "@capnp-js/write-data",
  "version": "0.4.0",
  "description": "Encode Cap'n Proto data.",
  "main": "lib/index.js",
  "scripts": {
    "build": "gulp build",
    "refresh": "rm -f package-lock.json && npm run clean && npm unpublish --force && rm -rf node_modules/@capnp-js/ && npm run build && npm publish",
    "clean": "gulp clean",
    "test-flow": "flow check --show-all-errors --strip-root",
    "test-unit": "nyc --cache=false --reporter=html --reporter=text mocha --require @babel/register test/unit/**/*.js",
    "test": "npm run test-flow && npm run test-unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/capnp-js/write-data.git"
  },
  "author": "Tim Popham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/capnp-js/write-data/issues"
  },
  "homepage": "https://github.com/capnp-js/write-data#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-flow-strip-types": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "@capnp-js/read-data": "^0.4.0",
    "babel-eslint": "^10.0.1",
    "del": "^3.0.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "flow-bin": "^0.89.0",
    "flow-typed": "^2.5.1",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-ext-replace": "^0.3.0",
    "gulp-jscc": "^1.2.0",
    "gulp-uglify": "^3.0.1",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0",
    "rollup": "^0.68.1"
  },
  "dependencies": {
    "@capnp-js/bytes": "^0.4.0",
    "@capnp-js/int64": "^0.4.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "8.9"
          },
          "modules": "commonjs"
        }
      ]
    ],
    "plugins": [
      "@babel/transform-flow-strip-types"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.js"
    ]
  }
}
